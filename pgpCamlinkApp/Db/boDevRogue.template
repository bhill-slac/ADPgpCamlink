# boDevRogue.template
record( bo, "$(P)$(R)$(NAME)" )
{
	field( DESC, "$(DESC=?)" )
	field( DTYP, "RogueBO" )
	field( DOL,  "$(DOL=)" )
	field( OUT,  "@B$(B) L$(L) $(PATH)" )
	field( ZNAM, "$(ZNAM=Disable)" )
	field( ONAM, "$(ONAM=Enable)" )
	field( PINI, "$(PINI=NO)" )
	info( autosaveFields, "$(SAVE=VAL)" )
}
